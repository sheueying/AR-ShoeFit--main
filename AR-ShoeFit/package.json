{
  "name": "shoe-try-on",
  "version": "1.0.0",
  "description": "DeepAR shoe-try-on demo.",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack.config.js --mode production",
    "postbuild": "node -e \"const fs=require('fs');const path=require('path');function copyDir(src,dest){fs.mkdirSync(dest,{recursive:true});fs.readdirSync(src,{withFileTypes:true}).forEach(entry=>{const srcPath=path.join(src,entry.name);const destPath=path.join(dest,entry.name);entry.isDirectory()?copyDir(srcPath,destPath):fs.copyFileSync(srcPath,destPath)})};copyDir('public','dist');copyDir('node_modules/deepar','dist/deepar-resources');\"",
    "dev": "webpack serve --mode development --open --port 8888"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.0"
  },
  "dependencies": {
    "bootstrap": "^5.3.2",
    "deepar": "^5.0.0",
    "express": "^5.1.0"
  }
}
